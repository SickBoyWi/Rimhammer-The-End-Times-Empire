<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<DoorsExpanded.DoorExpandedDef ParentName="HeronMetalDoorBase">
		<defName>RH_TET_Empire_BasicDoor</defName>
		<label>basic Empire door</label>
		<thingClass>DoorsExpanded.Building_DoorExpanded</thingClass>
		<description>A thick wooden door. Used by the people of the Empire that cannot afford a better door. It'll keep threats at bay for a short time, perhaps long enough for help to arrive.</description>
		<doorType>Standard</doorType>
		<altitudeLayer>DoorMoveable</altitudeLayer>
		<fillPercent>1</fillPercent>
		<statBases>
			<Mass>75</Mass>
			<MaxHitPoints>225</MaxHitPoints>
			<WorkToBuild>900</WorkToBuild>
		</statBases>
		<placeWorkers>
			<li>JecsTools.PlaceWorker_Outline</li>
		</placeWorkers>
		<stuffCategories>
			<li>Woody</li>
		</stuffCategories>
		<costStuffCount>35</costStuffCount>
		<graphicData>
			<texPath>Things/Buildings/Doors/RH_TET_Empire_DoorBasic_Mover</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>1</drawSize>
			<damageData>
				<enabled>false</enabled>
			</damageData>
		</graphicData>
		<building>
			<blueprintGraphicData>
				<texPath>Things/Buildings/Doors/RH_TET_Empire_DoorBasic_Blueprint</texPath>
				<drawSize>1</drawSize>
				<graphicClass>Graphic_Multi</graphicClass>
			</blueprintGraphicData>
		</building>
		<researchPrerequisites>
			<li>RH_TET_Empire_DoorsBasic</li>
		</researchPrerequisites>
		<uiIconPath>UI/Menu/RH_TET_Empire_DoorBasic_MenuIcon</uiIconPath>
		<designationHotKey>Misc1</designationHotKey>
		<blockLight>true</blockLight>
		<designationCategory>Structure</designationCategory>
	</DoorsExpanded.DoorExpandedDef>
  
	<DoorsExpanded.DoorExpandedDef ParentName="HeronMetalDoorBase">
