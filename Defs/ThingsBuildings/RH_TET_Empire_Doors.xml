			<WorkToBuild>1100</WorkToBuild>
		</statBases>
		<placeWorkers>
			<li>JecsTools.PlaceWorker_Outline</li>
		</placeWorkers>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>20</costStuffCount>
		<costList>
			<WoodLog>40</WoodLog>
		</costList>
		<graphicData>
			<texPath>Things/Buildings/Doors/RH_TET_Empire_DoorSturdy_Mover</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>1</drawSize>
			<shaderType>CutoutComplex</shaderType>
			<damageData>
				<enabled>false</enabled>
			</damageData>
		</graphicData>
		<building>
			<blueprintGraphicData>
				<texPath>Things/Buildings/Doors/RH_TET_Empire_DoorSturdy_Blueprint</texPath>
				<drawSize>1</drawSize>
				<graphicClass>Graphic_Multi</graphicClass>
