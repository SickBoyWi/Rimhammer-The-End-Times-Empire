	</DoorsExpanded.DoorExpandedDef>

	<DoorsExpanded.DoorExpandedDef ParentName="HeronMetalDoorBase">
		<defName>RH_TET_Empire_AdvancedDoor</defName>
		<label>strong Empire door</label>
		<thingClass>DoorsExpanded.Building_DoorExpanded</thingClass>
		<description>A very strong door built by the people of the Empire. Great for exterior doors. Intended to keep all manner of evil out. No lord's room or castle would be considered safe without such a door. Can be built of most any solid material, and painted is painted in appropriate colors.</description>
		<doorType>Standard</doorType>
		<altitudeLayer>DoorMoveable</altitudeLayer>
		<fillPercent>1</fillPercent>
		<statBases>
			<Mass>180</Mass>
			<Beauty>3</Beauty>
			<MaxHitPoints>400</MaxHitPoints>
			<WorkToBuild>1400</WorkToBuild>
		</statBases>
		<placeWorkers>
			<li>JecsTools.PlaceWorker_Outline</li>
		</placeWorkers>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>80</costStuffCount>
		<graphicData>
			<texPath>Things/Buildings/Doors/RH_TET_Empire_AdvancedDoor_Mover</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>1</drawSize>
			<shaderType>CutoutComplex</shaderType>
			<damageData>
				<enabled>false</enabled>
			</damageData>
		</graphicData>
		<building>
			<blueprintGraphicData>
				<texPath>Things/Buildings/Doors/RH_TET_Empire_AdvancedDoor_Blueprint</texPath>
				<drawSize>1</drawSize>
				<graphicClass>Graphic_Multi</graphicClass>
			</blueprintGraphicData>
		</building>
		<researchPrerequisites>
			<li>RH_TET_Empire_DoorsAdvanced</li>
		</researchPrerequisites>
		<uiIconPath>UI/Menu/RH_TET_Empire_AdvancedDoor_MenuIcon</uiIconPath>
		<designationHotKey>Misc1</designationHotKey>
		<blockLight>true</blockLight>
		<designationCategory>Structure</designationCategory>
	</DoorsExpanded.DoorExpandedDef>
  
	<DoorsExpanded.DoorExpandedDef ParentName="AbstractHeronDoorBase">
		<defName>RH_TET_Empire_BiggerDoor</defName>
		<label>Empire door (2x1)</label>
		<description>An Empire door made of wood and metal.\n\nLarger Empire doors are stronger, and have a variety of uses. They're used for the halls of heroes and lords, sally ports for castles, and wherever else a large strong door seems appropriate.</description>
		<size>(2,1)</size>
		<statBases>
			<Mass>250</Mass>
			<Beauty>1</Beauty>
			<MaxHitPoints>400</MaxHitPoints>
			<WorkToBuild>1500</WorkToBuild>
			<Flammability>1</Flammability>
		</statBases>
		<uiIconPath>UI/Menu/RH_TET_Empire_BiggerDoor_Menuicon</uiIconPath>
		<graphicData>
			<texPath>Things/Buildings/Doors/RH_TET_Empire_BiggerDoor_Mover</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(2,2)</drawSize>
			<shaderType>CutoutComplex</shaderType>
			<damageData>
				<enabled>false</enabled>
			</damageData>
		</graphicData>
		<doorType>DoubleSwing</doorType>
		<rotatesSouth>false</rotatesSouth>
		<fixedPerspective>true</fixedPerspective>
		<blockLight>true</blockLight>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costList>
			<WoodLog>75</WoodLog>
		</costList>
		<costStuffCount>40</costStuffCount>
		<researchPrerequisites>
			<li>RH_TET_Empire_DoorsBasic</li>
		</researchPrerequisites>
		<placeWorkers>
			<li>JecsTools.PlaceWorker_Outline</li>
		</placeWorkers>
		<rotatable>true</rotatable>
		<designationHotKey>Misc1</designationHotKey>
		<designationCategory>Structure</designationCategory>
	</DoorsExpanded.DoorExpandedDef>

	<DoorsExpanded.DoorExpandedDef ParentName="AbstractHeronDoorBase">
		<defName>RH_TET_Empire_StrongDoor</defName>
		<label>strong Empire door (2x1)</label>
		<description>A strong Empire door.\n\nThe highest quality Empire door for its size, this door can take some punishment; giving the men and women of the Empire time to prepare to repel whatever is attempting to break through.</description>
