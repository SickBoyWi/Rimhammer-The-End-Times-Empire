	<DoorsExpanded.DoorExpandedDef ParentName="AbstractHeronDoorBase">
		<defName>RH_TET_Empire_BasicGate</defName>
		<label>Empire gate (3x1)</label>
		<description>The gate to an Empire settlement. Sturdy; made of wood and metal.\n\nSafety from the horrors of the world requires not just sturdy walls, but a strong gate. The people of the Empire frequently are forced to hole up during sieges from found greenskins or the advances of choas armies.</description>
		<size>(3,1)</size>
		<statBases>
			<Mass>610</Mass>
			<Beauty>5</Beauty>
			<MaxHitPoints>650</MaxHitPoints>
			<WorkToBuild>3200</WorkToBuild>
			<Flammability>1</Flammability>
		</statBases>
		<uiIconPath>UI/Menu/RH_TET_Empire_SturdyGate_MenuIcon</uiIconPath>
		<uiIconScale>1.2</uiIconScale>
		<graphicData>
			<texPath>Things/Buildings/Doors/RH_TET_Empire_SturdyGate_Mover</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(3,4)</drawSize>
			<damageData>
				<enabled>false</enabled>
			</damageData>
		</graphicData>
		<doorType>DoubleSwing</doorType>
		<rotatesSouth>false</rotatesSouth>
		<fixedPerspective>true</fixedPerspective>
		<blockLight>true</blockLight>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>225</costStuffCount>
		<costList>
			<WoodLog>225</WoodLog>
		</costList>
		<researchPrerequisites>
