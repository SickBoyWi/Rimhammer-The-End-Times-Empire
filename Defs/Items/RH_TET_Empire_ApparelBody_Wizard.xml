<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="RH_TET_Empire_ApparelMakeableBase">
		<defName>RH_TET_Empire_WizardRobeLow</defName>
		<label>Empire robe</label>
		<description>A robe worn by the Empire wizards, and the faithful of Shallya.</description>
		<techLevel>Medieval</techLevel>
		<costStuffCount>100</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
		</stuffCategories>
		<graphicData>
			<texPath>Things/Items/WizardRobe/WizardRobeLow</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>2700</WorkToMake>
			<Mass>0.4</Mass>
			<StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>1.25</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.50</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>1.5</EquipDelay>
		</statBases>
		<thingCategories>
			<li>RH_TET_Empire_Apparel</li>
		</thingCategories>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Items/WizardRobe/WizardRobeLow</wornGraphicPath>
			<layers>
				<li>Middle</li>
			</layers>
			<tags>
				<li>RH_TET_Empire_Apparel_Wizard</li>
			</tags>
			<defaultOutfitTags>
				<li>RH_TET_Empire_WizardStandard</li>
				<li>RH_TET_Empire_WizardGreat</li>
			</defaultOutfitTags>
		</apparel>
		<recipeMaker>
			<researchPrerequisite>RH_TET_Empire_Apparel_Basic</researchPrerequisite>
			<skillRequirements>
				<Crafting>3</Crafting>
			</skillRequirements>
		</recipeMaker>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<only>(1.0,1.0,1.0,1)</only>
				</li>
				<li>
					<weight>10</weight>
					<only>(0.1,0.1,0.1,1)</only>
				</li>
				<li><!-- Red -->
					<weight>10</weight>
					<only>(.70,0.11,0.11,1)</only>
				</li>
				<li><!-- Green -->
					<weight>10</weight>
					<only>(0.22,.70,0.22,1)</only>
				</li>
				<li><!-- Blue -->
					<weight>10</weight>
					<only>(0.1,0.1,.73,1)</only>
				</li>
				<li><!-- Gray -->
					<weight>10</weight>
					<only>(.43,.43,.43,1)</only>
				</li>
				<li><!-- Yellow -->
					<weight>10</weight>
					<only>(.68,.68,0.04,1)</only>
				</li>
				<li><!-- Brown -->
					<weight>10</weight>
					<only>(0.39,0.22,0.05,1)</only>
				</li>
				<li><!-- Orange -->
					<weight>10</weight>
					<only>(.78,0.46,0.17,1)</only>
				</li>
			</options>
		</colorGenerator>
	</ThingDef>

