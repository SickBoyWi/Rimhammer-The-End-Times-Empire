<Defs>

	<!-- Food -->
	<RecipeDef>
		<defName>RH_TET_Empire_Bread</defName>
		<label>make bread</label>
		<description>Make bread from wheat. Tastes decent, provides good nutrition, and lasts a while without refrigeration.</description>
		<jobString>Making bread.</jobString>
		<workAmount>600</workAmount>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<requiredGiverWorkType>Cooking</requiredGiverWorkType>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>false</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>RH_TET_Empire_RawWheat</li>
					</thingDefs>
				</filter>
				<count>.5</count><!-- game multiplies this by 20 -->
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>RH_TET_Empire_RawWheat</li>
			</thingDefs>
		</fixedIngredientFilter>
		<recipeUsers>
			<li>ElectricStove</li>
			<li>FueledStove</li>
			<li>RH_TET_Empire_Hearth</li>
		</recipeUsers>
		<products>
			<RH_TET_Empire_Bread>1</RH_TET_Empire_Bread>
		</products>
		<workSkill>Cooking</workSkill>
		<skillRequirements>
			<Cooking>5</Cooking>
		</skillRequirements>
		<researchPrerequisite>RH_TET_Empire_Cooking</researchPrerequisite>
	</RecipeDef>

